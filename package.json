    "@pixi/display": "^6.2.1",
    "@pixi/graphics": "^6.2.1",
    "@pixi/math": "^6.2.1",
    "@pixi/runner": "^6.2.1",
    "@pixi/settings": "^6.2.1",
    "@pixi/sprite": "^6.2.1",
    "@pixi/ticker": "^6.2.1",
    "@pixi/unsafe-eval": "^6.2.1",
    "@pixi/utils": "^6.2.1",
    "@pixi/extensions": "^6.2.1",
    "ace-builds": "^1.4.12",
    "dompurify": "^2.3.1",
    "js-base64": "^3.7.2",
    "lodash": "^4.17.21",
    "marked": "^4.0.10",
    "pdfjs-dist": "^2.9.359"
  }
